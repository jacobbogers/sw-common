{
    "extends": "ts-node/node16/tsconfig.json",
    "compilerOptions": {
        // typescript options here
        "module": "ESNext",
        "moduleResolution":"node",
        "baseUrl": "./"

    },
    "ts-node":{
        "require": ["tsconfig-paths/register"],
        "compiler": "ttypescript",
        "compilerOptions":{
             // typescript options here
             "module": "commonjs",
             "allowJs": true,
             "jsx": "react",
             "baseUrl": "./"
        }
    }
}