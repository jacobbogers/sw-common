{
  "name": "femtologger",
  "version": "0.0.1-alpha",
  "description": "super tiny and fast logger inspired by debug",
  "repository": "/jacobbogers/sw-common",
  "main": "./dist/index.cjs",
  "type": "module",
  "exports": {
    "import": "./dist/index.mjs",
    "require": "./dist/index.cjs"
  },
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "sideEffects": true,
  "license": "MIT",
  "engines": {
    "node": ">=12.13"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "test": "npm run test-nodejs",
    "test-nodejs": "jest --env node",
    "build": "node scripts/build.js",
    "prepublishOnly": "npm run build",
    "prettier-check": "prettier --check .",
    "type-check": "tsc --noEmit",
    "precommit": "lint-staged"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "collectCoverage": true,
    "maxWorkers": "50%",
    "coveragePathIgnorePatterns": [
      "node_modules",
      "fixture.ts",
      "test"
    ]
  },
  "prettier": "@vercel/style-guide/prettier",
  "lint-staged": {
    "*": [
      "prettier --ignore-unknown --write"
    ],
    "*.{js,jsx,ts,tsx}": [
      "eslint --max-warnings=0 --fix"
    ]
  },
  "devDependencies": {
    "@types/debug": "4.1.7",
    "@types/jest": "27.0.1",
    "@vercel/style-guide": "3.0.0",
    "eslint": "8.12.0",
    "husky": "7.0.2",
    "jest": "27.1.1",
    "lint-staged": "11.1.2",
    "prettier": "2.6.2",
    "ts-jest": "27.0.5",
    "typescript": "4.6.3"
  },
  "dependencies": {
    "debug": "4.3.4"
  }
}
